        }
        else if ($('#AHeight').val().trim() == "") {
            customAlert("Height is required.");
            plainValidation('#AHeight');
        }
        else if ($('#AWeight').val().trim() == "") {
            customAlert("Weight is required.");
            plainValidation('#AWeight');
        }
        //else if ($('#HeadCircle').val().trim() == "") {
