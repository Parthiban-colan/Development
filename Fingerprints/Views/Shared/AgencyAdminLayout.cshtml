    <script type="text/javascript">
        $(document).ready(function () {
            $('input').keypress(function () {
                if ($(this).val() == 'MM/DD/YYYY') {
                    $(this).val('');
                    // $(this).mask("99/99/9999", { placeholder: 'MM/DD/YYYY' });
                }
            });

            //side menu height change//

            var footerHeight = $('.footer-one').height();
            var bodyHeight = $('body').height();
            var logoimageHeight = $('.logo-side-image').height();
            var sidemenu = $('#side-menu');
            var height = ((bodyHeight - (footerHeight + logoimageHeight)) - 7);
            sidemenu.height(height);

            //side menu height change//

            //Added on 1Jan2017
            startTimer();
            //End
            $("img").error(function () {
                $(this).hide();
            });
            $('#changePassword').click(function (e) {
                $('#currentPassword')[0].value = "";
                $('#newPassword')[0].value = "";
                $('#retypePassword')[0].value = "";
                //$('#Change_Pass').lightbox_me({ centered: true, onLoad: function () { } });
                e.preventDefault();
            });
            try {
                $("#searchAgencystaff").autocomplete({
                    minLength: 1,
                    source: function (request, response) {
                        $.ajax({
                            url: HostedDir + "/AgencyAdmin/AutoCompleteAgencystaff",
                            type: "POST",
                            dataType: "json",
                            data: { term: request.term },
                            success: function (data) {
                                response($.map(data, function (item) {
                                    return { label: item.FirstName, id: item.AgencyStaffId };
                                }))
                            }

                        });
                    },
                    select: function (event, ui) {
                        window.location.href = HostedDir + '/AgencyUser/editStaff/' + ui.item.id;
                        return false;
                    },
                    messages: {
                        noResults: "", results: ""
                    }
                })
            }
            catch (err) {
            }

            if('@ViewBag.Title'=="Roster")
            {
          
                try {
                    $("#SearchFamily").autocomplete({
                        search: function (event, ui) {

                            if ($("#SearchFamily").val().trim().length > 0 && $("#SearchFamily").val().length > 2) {
                                $('#Search').hide();
                                $('#Searchloader').show();
                            }
                        },
                        minLength: 1,
                        source: function (request, response) {
                            if ($("#SearchFamily").val().trim().length > 0 && $("#SearchFamily").val().length > 2) {

                                $.ajax({
                                    url: HostedDir + "/AgencyUser/AutoCompleteFamily",
                                    type: "POST",
                                    dataType: "json",
                                    data: { term: request.term },
                                    success: function (data) {
                                        $('#Search').show();
                                        $('#Searchloader').hide();
                                        response($.map(data, function (item) {
                                            return { label: item.Pfirstname, id: item.Encrypthouseholid };
                                        }))
                                    }

                                });
                            }
                        },
                        select: function (event, ui) {
                            window.location.href = HostedDir + '/AgencyUser/FamilySummary/' + ui.item.id;
                            //window.location.href = HostedDir + '/AgencyUser/FamilySummary';
                            return false;
                        },
                        messages: {
                            noResults: "", results: ""
                        }
                    })
                }
                catch (err) {
                }

            }
          


        });
        if (navigator.userAgent.match(/iPhone/i) || navigator.userAgent.match(/iPad/i)) {
            var viewportmeta = document.querySelector('meta[name="viewport"]');
            if (viewportmeta) {
                viewportmeta.content = 'width=device-width, minimum-scale=1.0, maximum-scale=1.0, initial-scale=1.0';
