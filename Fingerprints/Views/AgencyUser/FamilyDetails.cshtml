            mode = 1;
            text = 'Do you want to see all centers? ';
        }
        BootstrapDialog.confirm(text, function (result) {
            if (result) {

                $.ajax({
                    url: HostedDir + "/Agencyuser/Getallcenter",
                    type: "POST",
                    data: {
                        mode: mode
                    },
                    dataType: "json",
                    secureuri: false,
                    async: false,
                    success: function (response) {
                        if (response.length > 0) {
