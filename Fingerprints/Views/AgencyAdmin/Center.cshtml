                                                                        <input id="Saturday" type="checkbox" value="true" name="Classroom[@i].Saturday" data-val="true" />
                                                                    }
                                                                    <span>Saturday</span>
                                                                </label>
                                                            </div>
                                                            <div class="col-xs-6 col-sm-4">
                                                                <label class="checkbox-inline">
                                                                    @if (item.Sunday)
                                                                    {
                                                                        <input id="Sunday" type="checkbox" value="true" name="Classroom[@i].Sunday" data-val="true" />
                                                                    }
                                                                    else
                                                                    {
                                                                        <input id="Sunday" type="checkbox" value="true" name="Classroom[@i].Sunday" data-val="true" />
                                                                    }
                                                                    <span>Sunday</span>
                                                                </label>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                        }

                                        <div class="col-lg-6 col-md-6 col-sm-12">
                                            <div class="form-group">
                                                <label for="email">Number of seats</label>
                                                <input type="text" name="Classroom[@i].Noofseats" onblur="Checknumeric(this);" readonly="readonly" classid="@item.ClassroomID" class=" form-control" value="@item.Noofseats" id="Noofseats" placeholder="Number of Seats" maxlength="50">

                                            </div>
                                        </div>
                                        <div class="col-lg-6 col-md-6 col-sm-12">
                                            <div class="form-group">
                                                <label for="email">Actual Seats Used</label>
                                                <input type="text" name="Classroom[@i].ActualSeats" class="form-control" id="ActualSeats_@i" value="@item.ActualSeats" onblur="checkNoOfSeats(this);" placeholder="Actual Seats Used" maxlength="50" readonly="true">

                                            </div>
                                        </div>
                                        <div>
                                            <input type="hidden" value="@item.ClassroomID" name="Classroom[@i].ClassroomID">
                                        </div>

                                    </div>
                                    i++;
                                }
                            }
                        </div>
                    </div>



                </div>


                <div class="col-lg-6 col-md-12 col-sm-12">
                </div>


                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <div class="btnwrp_subcal">
                            @if (Session["MenuEnable"] != null && !Convert.ToBoolean(Session["MenuEnable"]))
                            {
                                <button class="my-btn" onclick="GoBackprofile();" type="button" tabindex="22">Back</button>
                            }

                            <button type="submit" class="my-btn" onclick="return isValidagencyInfo();" name="Command" value="SubmitCommand" tabindex="10">Submit</button>
                            <input id="CenterId" type="hidden" name="CenterId" value="@Model.CenterId" />
                            <button id="btnExit" class="my-btn" type="button" tabindex="11">Exit</button>
                            @if (Session["MenuEnable"] != null && !Convert.ToBoolean(Session["MenuEnable"]))
                            {

                                <button class="my-btn" onclick="CheckforCenters();" type="button" name="Command" value="NextCommand" tabindex="22">Next</button>
                            }

                            <div class="clear"></div>
                        </div>	<!--btnwrp_subcal--->
                    </div>
                </div>
            </form>


        </div><!--add-useragform--->
    </div>
}
