            if ((isNaN(inputTextObject.value)) && (len < 2)) {
                $(inputTextObject).val('');
                customAlert("Zip code must be numeric. ");
                var ret = false;
            }
            else if (len < 2) {
                $(inputTextObject).val('');
                customAlert("Zip code length must be 2 digit. ");
                var ret = false;
            }
            else if (isNaN(inputTextObject.value)) {
                $(inputTextObject).val('');
                customAlert("Zip code must be numeric. ");
