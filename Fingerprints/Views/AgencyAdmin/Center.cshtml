        function ucfirst(str, force) {
            str = force ? str.toLowerCase() : str;
            return str.replace(/(\b)([a-zA-Z])/,
                     function (firstLetter) {
                         return firstLetter.toUpperCase();
                     });
        }
        function Changefirstchar(value) {
            var cp_value = ucfirst($(value).val(), true);
            $(value).val(cp_value);
        }

        function hidediv() {
            if ('@ViewBag.mode' == "0") {
                if ($('#AdminSite').val() == "1") {
                    $('#divclass').hide();
                    $('#plussign').hide();
                    $('#Hideclassroomdiv').hide();
                    $('#HideHomeBased').hide();
                }
                if ($('#AdminSite').val() == "0") {
                    $('#divclass').show();
                    $('#plussign').show();
                    $('#Hideclassroomdiv').show();
                    $('#HideHomeBased').show();
                }
            }
            if ('@ViewBag.mode' == "1") {
                if ($('#AdminSite').val() == "1") {
                    BootstrapDialog.confirm('Do you want to select admin site? If you select admin site,classroom details gets deleted.', function (result) {
                        if (result) {
                            $('#AdminSite').val(1);
                            $.ajax({
                                url: HostedDir + "/AgencyAdmin/Deleteclassroom",
                                type: "POST",
                                data: {
                                    CenterId: $('#CenterId').val(),
                                    AgencyID: ""
                                },
                                dataType: "json",
                                secureuri: false,
                                async: false,
                                success: function (response) {
                                    if (response == 1) {
                                        customAlert("Admin Site selected successfully.");
                                        $('#divedit').hide();
                                        $('#plussign').hide();
                                        $('#Hideclassroomdiv').hide();
                                        $('#HideHomeBased').hide();
                                    }
                                }
                            , error: function (response) {
                                customAlert("Session Ended Log Onto The System Again."); setTimeout(function () { window.location.href = HostedDir + '/login/Loginagency'; }, 2000);
                            }
                            });
                        }
                        else {
                            $('#AdminSite').val(0);
                        }
                    });
                }
                if ($('#AdminSite').val() == "0") {
                    ShowDiv();
                    $('#plussign').show();
                    $('#Hideclassroomdiv').show();
                    $('#HideHomeBased').show();
                }
            }
        }
