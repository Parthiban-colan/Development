        <!-- Addres Change Model-->
        <div class="modal fade" id="myModalAddress" role="dialog" style="display: none;">
            <div class="modal-dialog">
                <!-- Modal content-->
                <div class="modal-content">
                    <div class="modal-header">
                        <input type="hidden" name="childid" id="childid" value="">
                        <input type="hidden" name="childname" id="childname" value="">
                        <label class="alert-dismissible" for="current_password" style="font-size:13px;">Your current Address is:<span class="parent-address"></span> </label>
                    </div>
                    <div class="form-group-al">
                        <div class="form-group input-container">
                            <label class="control-label" for="current_password">Date of Change </label>
                            <input name="CNotes" class="form-control txtAddressChangeDate txt-date txt-mandatory" id="CNotes" type="text" maxlength="50" placeholder="mm/dd/yyyy" value="">
                            <div class="err-container">
                                <label class="control-label"></label>
                                <span class="error-message error-message-empty">Date is mandatory</span>
                                <span class="error-message error-invalid-date">Invalid Date</span>
                                <span class="error-message error-future-date">Enter only future date</span>
                            </div>
                        </div>
                        <div class="form-group input-container">
                            <label class="control-label" for="current_password">Address </label>
                            <input name="CNotes" class="form-control txtAddress txt-mandatory" id="CNotes" type="text" maxlength="50" placeholder="Address" value="">
                            <div class="err-container">
                                <label class="control-label"></label>
                                <span class="error-message">Address is mandatory</span>
                            </div>
                        </div>
                        <div class="form-group input-container">
                            <label class="control-label" for="current_password">City </label>
                            <input name="CNotes" class="form-control txtAddressCity txt-mandatory" id="CNotes" type="text" maxlength="50" placeholder="City" value="">
                            <div class="err-container">
                                <label class="control-label"></label>
                                <span class="error-message">City is mandatory</span>
                            </div>
                        </div>
                        <div class="form-group input-container">
                            <label class="control-label" for="current_password">State </label>
                            @Html.DropDownList("ddlState", Fingerprints.Utilities.Helper.states(), new { @class = "form-control ddlState mandatory ddl-mandatory" })
                            <div class="err-container">
                                <label class="control-label"></label>
                                <span class="error-message">State is mandatory</span>
                            </div>
                        </div>
                        <div class="form-group input-container">
                            <label class="control-label" for="current_password">Zip Code</label>
                            <input name="CNotes" class="form-control txtZipCode txt-mandatory" id="CNotes" type="text" maxlength="50" placeholder="Zip Code" value="">
                            <div class="err-container">
                                <label class="control-label"></label>
                                <span class="error-message">Zip Code is mandatory</span>
                            </div>
                        </div>
                    </div>
                    <div class="modal-footer">

                        <button class="btnPassword address-btn btn-change-address">Change Address</button>
                        <button class="btnPassword address-btn" data-dismiss="modal">Cancel</button>
                    </div>
                </div>
            </div>
        </div>

