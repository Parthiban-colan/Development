    function Saveselectionpoint() {
        //rohit
        if (selectionobject != null) {

            if ($('#selectionpoint > tbody > tr').length > 0) {


                $(selectionobject).html($('#Grandpoint').val() + ' <i class="fa fa-arrow-up"></i>');
                var custompoints = [];
