        function RemoveClassroom(value) {

            if ($(value).attr("accesskey")) {
                if ($('#gridclassroom > tbody tr').length > 1) {
                    BootstrapDialog.confirm('Do you want to delete this classroom?', function (result) {
                        if (result) {
                            $.ajax({
                                url: HostedDir + "/AgencyAdmin/DeleteClassroomInfo",
                                type: "POST",
                                data: {
                                    ClassroomID: $(value).attr("accesskey"),
                                    CenterId: $('#CenterId').val(),
                                },
                                dataType: "json",
                                secureuri: false,
                                async: false,
                                success: function (response) {

                                    if (response == 1) {
                                        customAlert("Classroom deleted successfully.");
                                        rounds = 0;
                                        if ($('#gridclassroom > tbody tr').length > 1) {
                                            $(value).closest('tr').remove();
                                        }
                                        $('#gridclassroom > tbody tr').each(function () {
                                            var ClassName = "ClassRoom[" + rounds + "].ClassName";
                                            var Capacity = "ClassRoom[" + rounds + "].Capacity";
                                            var ClassroomID = "ClassRoom[" + rounds + "].ClassroomID";
                                            $(this).find('td:eq(0) :input[type="text"]').attr('name', ClassName);
                                            $(this).find('td:eq(1) :input[type="text"]').attr('name', Capacity);
                                            $(this).find('td:eq(2)').find(':input').attr('name', ClassroomID);
                                            rounds = rounds + 1;
                                        });
                                    }
                                }
                            , error: function (response) {
                                customAlert("Session Ended Log Onto The System Again."); setTimeout(function () { window.location.href = HostedDir + '/login/Loginagency'; }, 2000);
                            }
                            });
                        }
                    });
                }

            }
            else {
                rounds = 0;
                if ($('#gridclassroom > tbody tr').length > 1) {
                    $(value).closest('tr').remove();
                }
                $('#gridclassroom > tbody tr').each(function () {
                    var ClassName = "ClassRoom[" + rounds + "].ClassName";
                    var Capacity = "ClassRoom[" + rounds + "].Capacity";
                    var ClassroomID = "ClassRoom[" + rounds + "].ClassroomID";
                    $(this).find('td:eq(0) :input[type="text"]').attr('name', ClassName);
                    $(this).find('td:eq(1) :input[type="text"]').attr('name', Capacity);
                    $(this).find('td:eq(2)').find(':input').attr('name', ClassroomID);
                    rounds = rounds + 1;
                });
            }

        }


