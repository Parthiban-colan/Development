                        if (value == 1) {
                            if (response.Zipcodelist != null && response.Zipcodelist.length > 0) {
                                var option = "";
                                for (var i = 0; i < response.Zipcodelist.length; i++) {
                                    option = option + '<option value="' + response.Zipcodelist[i].City + '">' + response.Zipcodelist[i].City + '</option>';
